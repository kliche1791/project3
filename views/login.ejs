<head>
    <meta charset="utf-8">
    <title>MyNewsNow</title>
    <meta content="width=device-width, initial-scale=1.0" name="viewport">
    <meta content="" name="keywords">
    <meta content="" name="description">
  
  
    <link href="https://fonts.googleapis.com/css?family=Montserrat:300,400,500,700|Open+Sans:300,300i,400,400i,700,700i" rel="stylesheet">
    <link href="lib/bootstrap/css/bootstrap.min.css" rel="stylesheet">
    <link href="lib/animate/animate.min.css" rel="stylesheet">
    <link href="css/style.css" rel="stylesheet">
  
    
  </head>
<body>
    <section id="intro">
    <div class="intro-textlog-reg">
        <h2>Welcome to MyNewsNow</h2>
        <h1>Login</h1>
        <% if (messages.error) { %>
            <%= messages.error %>
          <% } %>
          <form action="/login" method="POST" >
            <div>
             <label for="email" style="margin-right: 85px;"><h1>Email</h1></label> 
              <input type="email" id="email" name="email" required>
            </div>
            <div>
             <label for="password"><h1>Password</h1></label>
              <input type="password" id="password" name="password" required>
            </div>
            <button type="submit" class="btn-get-started scrollto" >Login</button>
          </form>
         <h1> <a href="/register" style=" color: inherit;">Register</a> </h1>
      </div>
    </section>


  <!-- JavaScript Libraries -->
  <script src="lib/jquery/jquery.min.js"></script>
  <script src="lib/jquery/jquery-migrate.min.js"></script>
  <script src="lib/bootstrap/js/bootstrap.bundle.min.js"></script>
  <script src="lib/easing/easing.min.js"></script>
  <script src="lib/wow/wow.min.js"></script>
  <script src="lib/superfish/hoverIntent.js"></script>
  <script src="lib/superfish/superfish.min.js"></script>
  <script src="lib/magnific-popup/magnific-popup.min.js"></script>

  
  <script src="contactform/contactform.js"></script>

  
  <script src="js/main.js"></script>

</body>